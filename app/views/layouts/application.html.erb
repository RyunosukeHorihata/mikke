<!DOCTYPE html>
<html>
  <head>
    <title>mikke</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="de-header">
      <div class="de-header2">
        <%= link_to 'mikke', '/', class: "de-header_title"%>
      </div>
      <div class="de-header3">
        <div class="de-header3-search">
          <div class="de-header3-search2">
            <%= form_with(url: search_account_path, local: true, method: :get, class: "search-form") do |form| %>
              <%= form.text_field :keyword, placeholder: "投稿を検索する", class: "search-input" %>
              <%= form.submit "検索", class: "search-btn" %>
            <% end %>
            <%# <input placeholder="キーワードで検索" type="search" class="de-header3-search_form-box" style= " -webkit-appearance: none; width:870px;height:30px;">
            <input type="submit" value="検索" class="de-header3-search_form-btn" style= " -webkit-appearance: none; width:100px;height:30px;"> %>
          </div>
        </div>
      </div>
    </div>
    <%= yield %>
  </body>
</html>
